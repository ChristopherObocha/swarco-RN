{
  "name": "swarco-econnect-react-native",
  "version": "4.0.0",
  "environment": "development",
  "possibleEnvironment": [
    "development",
    "staging",
    "production"
  ],
  "private": true,
  "scripts": {
    "setup": "source import-env-vars.sh",
    "sofy": "source appcenter-post-build.sh",
    "start": "react-native start --reset-cache",
    "android": "tsc && react-native run-android",
    "ios": "tsc && npx react-native setup-ios-permissions && cd ios && pod install && cd .. && react-native run-ios --simulator=\"iPhone Xs\"",
    "ios-oldarch-prep-run-once": "cd ios && bundle install && RCT_NEW_ARCH_ENABLED=0 bundle exec pod install && cd ..",
    "ios-newarch-prep-run-once": "cd ios && bundle install && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install && cd ..",
    "clear-all-android": "rm -rf android/app/build android/build",
    "clear-all-ios": "rm -rf ios/Podfile.lock ios/Pods ~/Library/Developer/Xcode/DerivedData ios/build && pod cache clean --all",
    "postinstall": "patch-package",
    "lint": "eslint .",
    "test": "jest"
  },
  "dependencies": {
    "@apollo/client": "^3.8.10",
    "@georstat/react-native-image-gallery": "^1.1.0",
